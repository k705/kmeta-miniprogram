/* button */
.t-button--size-small {
  font-weight:400!important;
}
/* tag */
.t-tag--light.t-tag--warning {
  border: 0;
}
/* DropdownMenu 下拉菜单 */
.t-icon-caret-down-small{
  font-size:24rpx!important;
  /* margin-left:12rpx; */
}
.t-dropdown-menu__title{
  margin-right:12rpx;
}
.t-dropdown-menu {
  height: 98rpx !important;
  padding: 0 30rpx !important;
}
.t-dropdown-item__body .t-icon-check{
  width: 32rpx;
  height: 32rpx;
  font-size: 32rpx;
}
.t-dropdown-menu__item {
  padding: 0 !important;
}
.t-dropdown-item__scroll .t-radio {
  flex-direction: row-reverse;
}
.t-radio--block {
  height: 42rpx;
  box-sizing: content-box;
}

.t-dropdown-item .t-radio__title {
  -webkit-line-clamp: 1 !important;
}
/* popup */
.t-popup--top {
  border-bottom-left-radius: 0 !important;
  border-bottom-right-radius: 0 !important;
}
.t-popup__close {
  padding: 12rpx 32rpx !important;
  position: absolute;
  z-index: 10;
}
.popup--bottom {
  padding: 90rpx 32rpx 0;
  box-sizing: border-box;
  background-color: #fff;
  margin: 32rpx 0 0;
  width: 100vw;
}
.popup-title {
  color: #333;
  font-size: 32rpx;
  font-weight: 500;
  position: absolute;
  top: 0rpx;
  left: 32rpx;
  height: 80rpx;
  right: 32rpx;
  background-color: #fff;
  z-index: 9;
}
.popup-sub-title {
  color: #333;
  font-size: 28rpx;
  font-weight: 500;
  margin-bottom: 24rpx;
}
/* textarea */
.t-textarea {
  padding: 24rpx !important;
}
.t-textarea__indicator:not(:empty) {
  padding-top:24rpx!important;
}
/* tabs */
.t-tabs__track {
  background-color: transparent;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  bottom: 10rpx !important;
}
.t-tabs__item {
  padding: 0 16rpx !important;
}
.t-tabs__item-inner {
  font-size: 32rpx;
}
.t-tabs__item--active {
  font-weight: 700 !important;
}
/* step */
.step-middle {
  /* background-color: red; */
}
.step-middle .t-steps-item__dot {
  width: 19rpx !important;
  height: 19rpx !important;
  box-sizing: border-box;
}
.step-middle .t-steps-item__anchor--vertical {
  width: 20rpx !important;
  height: 20rpx !important;
}

.step-middle .t-steps-item__content--vertical {
  margin-left: 24rpx !important;
  background-color: #fff;
  padding: 24rpx;
  margin-bottom: 24rpx;
}
.step-middle .t-steps-item__line--vertical {
  width: 1rpx !important;
}
.step-middle .step-middle.t-steps-item__dot {
  border-width: 1rpx !important;
}
.step-middle .t-steps-item__dot--default {
  background-color: #f8f8f8 !important;
}
.step-middle .t-steps-item__line--vertical.t-steps-item__line--dot {
  top: 50% !important;
}
.step-middle .t-steps-item__dot {
  position: absolute;
  top: calc(50% - 10rpx);
  z-index: 1;
}
.step-middle .t-steps-item__content--vertical.t-steps-item__content--last {
  padding-bottom: 32rpx !important;
}
.step-middle .t-steps-item__title--vertical {
  font-weight: 500 !important;
}
.step-middle .t-steps-item {
  position: relative;
}
.step-middle .t-steps-item:after {
  position: absolute;
  content: '';
  left: 10rpx;
  top: 0;
  bottom: 0;
  width: 1rpx;
  height: 100%;
  background-color: #ccc;
}
.step-middle .t-steps-item:first-child::after {
  top: 50%;
}
.step-middle .t-steps-item:last-child::after {
  height: 50%;
}
.step-middle .t-steps-item__line {
  background-color: transparent !important;
}

.step-top .t-steps-item__extra:not(:empty) {
  margin-top: 0 !important;
}
.step-top .t-steps-item__dot {
  width: 16rpx !important;
  height: 16rpx !important;
  box-sizing: border-box;
  z-index: 9;
}
.step-top .t-steps-item__anchor--vertical {
  width: 16rpx !important;
  height: 16rpx !important;
  margin-top: 27rpx;
}
.step-top .t-steps-item__content--vertical {
  margin-left: 16rpx !important;
  background-color: #fff;
}
.step-top .t-steps-item__line--vertical {
  width: 1rpx !important;
}
.step-top .step-middle.t-steps-item__dot {
  border-width: 1rpx !important;
}
.step-top .t-steps-item__dot--default {
  background-color: #f8f8f8 !important;
}
.step-top .t-steps-item__line--vertical.t-steps-item__line--dot {
  top: calc(7% + 24rpx) !important;
  height: calc(100%);
}
.step-top .t-steps-item {
  padding-left: 16rpx;
  /* padding-top: 16rpx; */
}
.t-steps-item__dot--process {
  background-color: #fff !important;
}
/* process */
.my-process.t-progress--over-ten .t-progress__info {
  top: 30rpx !important;
  right: 0 !important;
}
.my-process.t-progress--plump .t-progress__info {
  font-size: 20rpx !important;
}

.my-process.t-progress__bar {
  overflow: visible !important;
}
